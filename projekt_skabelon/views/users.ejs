<% include partials/head.ejs %>


    <h1>Users</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(result =>{%>
            <tr>
                <td><%= result.name %></td>
                <td><%= result.username %></td>
                <td><%= result.email %></td>
                <td><a href="/editUser/<%= result.id %>">Edit</a></td>
                <td><a href="/deleteuser/<%= result.id %>">Delete</a></td>
            </tr>
            <%})%>
        </tbody>
    </table>
    <% include partials/footer.ejs %>